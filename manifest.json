{
    "manifest_version": 2,
    "name": "Fast PiHole Statistics",
    "version": "1.0",
    "description": "Fetches data from an API and displays it in a popup",
    "homepage_url": "https://github.com/jlavery34/pistats",
    "permissions": [
        "storage",
        "activeTab",
        "http://192.168.*.*",
        "https://192.168.*.*"
      ],      
    "browser_action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "48": "icons/pihole48.png"
      }
    },
    "icons": {
      "48": "icons/pihole48.png"
    }
  }
  